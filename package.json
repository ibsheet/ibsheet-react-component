{
  "name": "ibsheet-react-component",
  "private": true,
  "version": "1.0.3",
  "license": "MIT",
  "type": "module",
  "types": "dist/ibsheet-react/index.d.ts",
  "main": "dist/ibsheet-react/ibsheet-react.cjs.js",
  "module": "dist/ibsheet-react/ibsheet-react.es.js",
  "exports": {
    ".": {
      "types": "./dist/ibsheet-react/index.d.ts",
      "import": "./dist/ibsheet-react/ibsheet-react.es.js",
      "require": "./dist/ibsheet-react/ibsheet-react.cjs.js"
    }
  },
  "files": [
    "dist/ibsheet-react"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --project tsconfig.build.json && yarn copy-package",
    "copy-package": "node scripts/copy-package.cjs",
    "postbuild": "shx cp src/README.md LICENSE CHANGELOG.md dist/ibsheet-react/",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:test": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier \"**/*.{js,jsx,ts,tsx,json,css,md}\" --list-different",
    "fix:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "fix:prettier": "prettier \"**/*.{js,jsx,ts,tsx,json,css,md}\" --write --list-different",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'",
    "preview": "vite preview",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:publish": "yarn release && cd dist/ibsheet-react && npm publish --access public"
  },
  "dependencies": {
    "@ibsheet/interface": "^1.0.0",
    "@types/prop-types": "^15.7.15",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@typescript-eslint/utils": "^8.39.1",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.4.0",
    "standard-version": "^9.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
